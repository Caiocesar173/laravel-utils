{
    "name": "caiocesar173/laravel-utils",
    "description": "Utils Laravel Package",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.4",
        "laravel/passport": "^11.2",
        "laravel/socialite": "^5.5",
        "laravel/ui": "^4.0",
        "lucascudo/laravel-pt-br-localization": "^1.2",
        "owen-it/laravel-auditing": "^13.0",
        "prettus/l5-repository": "^2.7",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-translatable": "^6.0",
        "stevebauman/location": "^6.2",
        "watson/validating": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Caiocesar173\\Utils\\": "src/"
        }     
    },
    "autoload-dev": {
        "psr-4": {
            "Caiocesar173\\Utils\\": "src/"
        },
        "classmap": [
            "Database/Seeders"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caiocesar173\\Utils\\Providers\\UtilsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "caiocesar173",
            "email": "caiocesar173caiocesar173@gmail.com"
        }
    ]
}
